{
  "name": "docco-next",
  "description": "Literate programming parser",
  "keywords": [
    "documentation",
    "generator",
    "literate",
    "literate programming"
  ],
  "scripts": {
    "docs": "bin/docco -o .  -l plain -x md  ./docco.js;mv docco.md README.md;bin/docco -o docs  docco.js;mv docs/docco.html docs/index.html"
  },
